<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragmentss.ConditionFormFourFrgmnt">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/screen_bg_hash_color">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_margin="15dp">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <Button
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:minHeight="45dp"
                            android:text="Super Structure"
                            android:textColor="@color/white"
                            android:textSize="@dimen/sub_header_text_s"
                            android:textAllCaps="true"
                            android:background="@drawable/rounded_corner_for_login_bttn"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="@dimen/common_header_mT"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical">

                                <com.spiderindia.departmentsofhighway.Utils.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@null"
                                    android:text="Concrete"
                                    android:textAllCaps="false"
                                    android:textColor="@color/half_black"
                                    android:textSize="@dimen/common_header_txt_s" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center">

                                <com.spiderindia.departmentsofhighway.Utils.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@null"
                                    android:text="Steel"
                                    android:textAllCaps="false"
                                    android:textColor="@color/half_black"
                                    android:textSize="@dimen/common_header_txt_s" />


                            </LinearLayout>

                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="7dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/cracked_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Cracked"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                       android:id="@+id/leached_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Leached"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/saliyo_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Saliyo"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/spality_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Spality"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/corrosion_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Corrosion"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/buckled_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="buckled"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">
                                <com.spiderindia.departmentsofhighway.Utils.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Arch"
                                    android:textColor="@color/half_black"
                                    android:textSize="@dimen/common_header_txt_s"
                                    android:textAllCaps="false"
                                    android:background="@null"
                                    android:gravity="center_vertical"
                                    android:layout_marginBottom="5dp"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/spalled_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Spalled"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/vegetation_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Vegetation"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                        android:id="@+id/scaled_chkBx_superS"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layoutDirection="rtl"
                                        android:text="Scaled"
                                        android:textAllCaps="true"
                                        android:textColor="@color/txt_common_color"
                                        android:textDirection="ltr"
                                        android:textSize="@dimen/common_header_txt_s" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                            </LinearLayout>

                        </LinearLayout>

                        <!--mimfbmnfiod-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/common_header_mB">

                            <Button
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:minHeight="45dp"
                                android:text="Hand Rails"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sub_header_text_s"
                                android:textAllCaps="true"
                                android:background="@drawable/rounded_corner_for_login_bttn"/>

                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/common_header_mT"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                    android:id="@+id/broken_chkBx_hand_superS"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="10dp"
                                    android:layoutDirection="rtl"
                                    android:text="Broken"
                                    android:textAllCaps="true"
                                    android:textColor="@color/txt_common_color"
                                    android:textDirection="ltr"
                                    android:textSize="@dimen/common_header_txt_s"
                                    />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                    android:id="@+id/corrded_chkBx_hand_superS"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="10dp"
                                    android:layoutDirection="rtl"
                                    android:text="Corroded"
                                    android:textAllCaps="true"
                                    android:textColor="@color/txt_common_color"
                                    android:textDirection="ltr"
                                    android:textSize="@dimen/common_header_txt_s" />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1.4"
                                android:gravity="center_vertical">


                                <com.spiderindia.departmentsofhighway.Utils.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Spalling"
                                    android:textAllCaps="true"
                                    android:textColor="@color/txt_common_color"
                                    android:textSize="@dimen/common_header_txt_s" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical">

                                <Spinner
                                    android:id="@+id/spalling_spnr_superS"
                                    android:layout_width="110dp"
                                    android:layout_height="35dp"
                                    android:background="@drawable/rounded_corner_with_bottom_shadow"
                                    android:gravity="center_vertical"
                                    android:popupBackground="@color/white"
                                    android:theme="@android:style/Theme.Holo.Light" />

                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/common_header_mB">

                            <Button
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:minHeight="45dp"
                                android:text="FootPath"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sub_header_text_s"
                                android:textAllCaps="true"
                                android:background="@drawable/rounded_corner_for_login_bttn"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/common_header_mT"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                    android:id="@+id/broken_foot_chkBx_superS"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="10dp"
                                    android:layoutDirection="rtl"
                                    android:text="Broken"
                                    android:textAllCaps="true"
                                    android:textColor="@color/txt_common_color"
                                    android:textDirection="ltr"
                                    android:textSize="@dimen/common_header_txt_s"
                                    />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <com.spiderindia.departmentsofhighway.Utils.CustomFontCheckBox
                                    android:id="@+id/disintegration_foot_chkBx_superS"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="10dp"
                                    android:layoutDirection="rtl"
                                    android:text="Disintegration No"
                                    android:textAllCaps="true"
                                    android:textColor="@color/txt_common_color"
                                    android:textDirection="ltr"
                                    android:textSize="@dimen/common_header_txt_s" />

                            </LinearLayout>



                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:minHeight="@dimen/common_header_min_H"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/nxn_prev_lay_mar_t"
                        android:gravity="center">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <Button
                                android:id="@+id/previous_bttn"
                                android:layout_width="wrap_content"
                                android:layout_height="35dp"
                                android:text="Previous"
                                android:textColor="@color/white"
                                android:textSize="@dimen/bottom_text_s"
                                android:textAllCaps="true"
                                android:paddingLeft="8dp"
                                android:paddingRight="8dp"
                                android:layout_alignParentLeft="true"
                                android:layout_centerInParent="true"
                                android:background="@color/bttn_common_color"/>
                            <Button
                                android:id="@+id/nxt_btn"
                                android:layout_width="wrap_content"
                                android:layout_height="35dp"
                                android:text="Next"
                                android:textColor="@color/white"
                                android:textSize="@dimen/bottom_text_s"
                                android:textAllCaps="true"
                                android:paddingLeft="8dp"
                                android:paddingRight="8dp"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:background="@color/bttn_common_color"/>
                        </RelativeLayout>


                    </LinearLayout>
                </LinearLayout>
            </ScrollView>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/progress_loading_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:visibility="gone">

        <ProgressBar
            android:id="@+id/process_Loading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</RelativeLayout>
